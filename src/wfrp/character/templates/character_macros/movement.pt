<table id="movement">
    <thead>
        <tr>
            <th colspan="6">MOVEMENT</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>Movement</td>
            <td>${character.movement}</td>
            <td>Walk</td>
            <td>${character.movement * 2}</td>
            <td>Run</td>
            <td>${character.movement * 4}</td>
        </tr>
    </tbody>
</table>
