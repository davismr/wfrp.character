<div id="movement" class="border">
    <div class="row">
        <div class="col-md-12 caption">Movement</div>
    </div>
    <div class="row">
        <div class="col-md-2">Movement</div>
        <div class="col-md-2">${character.movement}</div>
        <div class="col-md-2">Walk</div>
        <div class="col-md-2">${character.movement * 2}</div>
        <div class="col-md-2">Run</div>
        <div class="col-md-2">${character.movement * 4}</div>
    </div>
</div>
