<table id="wealth">
    <thead>
        <tr>
            <th colspan="6" class="section-header">Weapons</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>Name</td>
            <td>Group</td>
            <td>Enc</td>
            <td>Range/Reach</td>
            <td>Damage</td>
            <td>Qualities</td>
        </tr>
        <tr tal:repeat="weapon python:character.weapons">
            <td>${weapon}</td>
            <td>${weapons_data[weapon]["Group"]}</td>
            <td>${weapons_data[weapon]["Enc"]}</td>
            <td>${weapons_data[weapon]["Reach"]}</td>
            <td>TODO ${weapons_data[weapon]["Damage"]}</td>
            <td>${weapons_data[weapon]["Qualities"]|"&nbsp;"}</td>
        </tr>
    </tbody>
</table>
