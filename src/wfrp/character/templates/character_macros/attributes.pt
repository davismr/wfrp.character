<div id="attributes" class="border">
    <div class="row caption">
        <div class="col-md-12">Characteristics</div>
    </div>
    <div class="row">
        <div class="col-md-2">&nbsp;</div>
        <div class="col-md-1 center">WS</div>
        <div class="col-md-1 center">BS</div>
        <div class="col-md-1 center">S</div>
        <div class="col-md-1 center">T</div>
        <div class="col-md-1 center">I</div>
        <div class="col-md-1 center">Ag</div>
        <div class="col-md-1 center">Dex</div>
        <div class="col-md-1 center">Int</div>
        <div class="col-md-1 center">WP</div>
        <div class="col-md-1 center">Fel</div>
    </div>
    <div class="row">
        <div class="col-md-2">Initial</div>
        <div class="col-md-1 center">${character.weapon_skill_initial}</div>
        <div class="col-md-1 center">${character.ballistic_skill_initial}</div>
        <div class="col-md-1 center">${character.strength_initial}</div>
        <div class="col-md-1 center">${character.toughness_initial}</div>
        <div class="col-md-1 center">${character.initiative_initial}</div>
        <div class="col-md-1 center">${character.agility_initial}</div>
        <div class="col-md-1 center">${character.dexterity_initial}</div>
        <div class="col-md-1 center">${character.intelligence_initial}</div>
        <div class="col-md-1 center">${character.willpower_initial}</div>
        <div class="col-md-1 center">${character.fellowship_initial}</div>
    </div>
    <div class="row">
        <div class="col-md-2">Advances</div>
        <div class="col-md-1 center">${character.weapon_skill_advances}</div>
        <div class="col-md-1 center">${character.ballistic_skill_advances}</div>
        <div class="col-md-1 center">${character.strength_advances}</div>
        <div class="col-md-1 center">${character.toughness_advances}</div>
        <div class="col-md-1 center">${character.initiative_advances}</div>
        <div class="col-md-1 center">${character.agility_advances}</div>
        <div class="col-md-1 center">${character.dexterity_advances}</div>
        <div class="col-md-1 center">${character.intelligence_advances}</div>
        <div class="col-md-1 center">${character.willpower_advances}</div>
        <div class="col-md-1 center">${character.fellowship_advances}</div>
    </div>
    <div class="row">
        <div class="col-md-2">Advances</div>
        <div class="col-md-1 center">${character.weapon_skill}</div>
        <div class="col-md-1 center">${character.ballistic_skill}</div>
        <div class="col-md-1 center">${character.strength}</div>
        <div class="col-md-1 center">${character.toughness}</div>
        <div class="col-md-1 center">${character.initiative}</div>
        <div class="col-md-1 center">${character.agility}</div>
        <div class="col-md-1 center">${character.dexterity}</div>
        <div class="col-md-1 center">${character.intelligence}</div>
        <div class="col-md-1 center">${character.willpower}</div>
        <div class="col-md-1 center">${character.fellowship}</div>
    </div>
</div>
