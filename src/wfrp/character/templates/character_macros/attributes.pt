<div id="attributes" class="border">
    <div class="row caption">
        <div class="col-md-12">Characteristics</div>
    </div>
    <div class="row">
        <div class="col-md-2">&nbsp;</div>
        <div class="col-md-1">WS</div>
        <div class="col-md-1">BS</div>
        <div class="col-md-1">S</div>
        <div class="col-md-1">T</div>
        <div class="col-md-1">I</div>
        <div class="col-md-1">Ag</div>
        <div class="col-md-1">Dex</div>
        <div class="col-md-1">Int</div>
        <div class="col-md-1">WP</div>
        <div class="col-md-1">Fel</div>
    </div>
    <div class="row">
        <div class="col-md-2">Initial</div>
        <div class="col-md-1">${character.weapon_skill_initial}</div>
        <div class="col-md-1">${character.ballistic_skill_initial}</div>
        <div class="col-md-1">${character.strength_initial}</div>
        <div class="col-md-1">${character.toughness_initial}</div>
        <div class="col-md-1">${character.initiative_initial}</div>
        <div class="col-md-1">${character.agility_initial}</div>
        <div class="col-md-1">${character.dexterity_initial}</div>
        <div class="col-md-1">${character.intelligence_initial}</div>
        <div class="col-md-1">${character.willpower_initial}</div>
        <div class="col-md-1">${character.fellowship_initial}</div>
    </div>
    <div class="row">
        <div class="col-md-2">Advances</div>
        <div class="col-md-1">${character.weapon_skill_advances}</div>
        <div class="col-md-1">${character.ballistic_skill_advances}</div>
        <div class="col-md-1">${character.strength_advances}</div>
        <div class="col-md-1">${character.toughness_advances}</div>
        <div class="col-md-1">${character.initiative_advances}</div>
        <div class="col-md-1">${character.agility_advances}</div>
        <div class="col-md-1">${character.dexterity_advances}</div>
        <div class="col-md-1">${character.intelligence_advances}</div>
        <div class="col-md-1">${character.willpower_advances}</div>
        <div class="col-md-1">${character.fellowship_advances}</div>
    </div>
    <div class="row">
        <div class="col-md-2">Advances</div>
        <div class="col-md-1">${character.weapon_skill}</div>
        <div class="col-md-1">${character.ballistic_skill}</div>
        <div class="col-md-1">${character.strength}</div>
        <div class="col-md-1">${character.toughness}</div>
        <div class="col-md-1">${character.initiative}</div>
        <div class="col-md-1">${character.agility}</div>
        <div class="col-md-1">${character.dexterity}</div>
        <div class="col-md-1">${character.intelligence}</div>
        <div class="col-md-1">${character.willpower}</div>
        <div class="col-md-1">${character.fellowship}</div>
    </div>
</div>
